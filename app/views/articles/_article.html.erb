<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= article.author %>
  </p>

  <p>
    <strong>Article body:</strong>
    <%= article.article_body %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= article.category %>
  </p>

  <p>
    <strong>Published:</strong>
    <%= article.published %>
  </p>

  <p>
    <strong>Photo:</strong>
    <%= article.photo %>
  </p>

</div>
